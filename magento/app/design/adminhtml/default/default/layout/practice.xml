<?xml version="1.0"?>
<layout>
     <practice_adminhtml_practice_attribute_edit>
        <reference name="left">
            <block type="practice/adminhtml_practice_attribute_edit_tabs" name="practice_edit_tabs"></block>
        </reference>
        <reference name="content">
            <block type="practice/adminhtml_practice_attribute_edit" name="practice_edit_content"></block>
        </reference>
        <reference name="js">
            <block type="adminhtml/template" name="attribute_edit_js" template="practice/attribute/js.phtml"></block>
        </reference>
    </practice_adminhtml_practice_attribute_edit>


    <practice_adminhtml_practice_edit>
        <update handle="editor"/>
        <reference name="content">
            <block type="practice/adminhtml_practice_edit" name="practice_edit"></block>
        </reference>
        <reference name="left">
            <block type="practice/adminhtml_practice_edit_tabs" name="practice_tabs"></block>
        </reference>
        <reference name="js">
            <block type="adminhtml/catalog_product_edit_js" template="catalog/product/js.phtml" name="catalog_product_js"></block>
            <block type="core/template" template="catalog/wysiwyg/js.phtml"/>
        </reference>
    </practice_adminhtml_practice_edit>
</layout>
